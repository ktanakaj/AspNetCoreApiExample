{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:30913",
      "sslPort": 0
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "EXAMPLEAPP_SERILOG__WRITETO__0__ARGS__CONFIGURELOGGER__WRITETO__0__ARGS__PATH": "%TEMP%\\AspNetCoreApiExample\\app.log",
        "EXAMPLEAPP_DATABASE__TYPE": "InMemory",
        "EXAMPLEAPP_SERILOG__WRITETO__2__ARGS__CONFIGURELOGGER__WRITETO__0__ARGS__PATH": "%TEMP%\\AspNetCoreApiExample\\sql.log",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "EXAMPLEAPP_SERILOG__WRITETO__1__ARGS__CONFIGURELOGGER__WRITETO__0__ARGS__PATH": "%TEMP%\\AspNetCoreApiExample\\access.log"
      }
    },
    "AspNetCoreApiExample": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/blogs",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true
    }
  }
}